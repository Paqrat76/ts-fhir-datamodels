{
  "name": "@paq-ts-fhir/fhir-core",
  "version": "1.1.0",
  "description": "FHIR core package required by generated FHIR data model packages in @paq-ts-fhir",
  "keywords": [
    "node",
    "typescript",
    "FHIR",
    "models",
    "data models",
    "fhir-core"
  ],
  "author": "Joe Paquette <paqrat76@gmail.com>",
  "homepage": "https://github.com/Paqrat76/ts-fhir-datamodels/blob/main/packages/fhir-core/README.md",
  "bugs": {
    "url": "https://github.com/Paqrat76/ts-fhir-datamodels/issues",
    "email": "paqrat76@gmail.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**",
    "CHANGELOG.md",
    "!lib/__test__/**"
  ],
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo lib coverage",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "jest",
    "precoverage": "rm -rf coverage",
    "coverage": "jest --coverage",
    "postcoverage": "opener ./coverage/lcov-report/index.html",
    "jest:clear": "jest --clearCache"
  },
  "dependencies": {
    "luxon": "^3.7.1",
    "zod": "^4.1.5"
  },
  "engines": {
    "node": ">=22.11",
    "npm": ">=10.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github:Paqrat76/ts-fhir-datamodels/ts-fhir-datamodels.git",
    "directory": "packages/fhir-core"
  },
  "publishConfig": {
    "access": "public"
  }
}
