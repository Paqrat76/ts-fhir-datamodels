{
  "name": "generator-datamodel-ftest",
  "private": true,
  "version": "0.4.1",
  "description": "Generated FHIR R4 data models specifically for functional testing",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo dist coverage src/*",
    "generate:ftest": "ts-node scripts/generate-ftest-src.ts",
    "precompile": "npm run generate:ftest",
    "compile": "tsc",
    "pretest:func": "npm run jest:clear && npm run generate:ftest",
    "test:func": "jest",
    "precoverage:func": "npm run jest:clear && npm run generate:ftest",
    "coverage:func": "jest --coverage",
    "jest:clear": "jest --clearCache",
    "jest:updateSnapshot": "jest --updateSnapshot"
  },
  "dependencies": {
    "@paq-ts-fhir/fhir-core": "^0.6.2"
  },
  "engines": {
    "node": ">=22.11",
    "npm": ">=10.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github:Paqrat76/ts-fhir-datamodels/ts-fhir-datamodels.git",
    "directory": "app/generator-datamodel-ftest"
  }
}
